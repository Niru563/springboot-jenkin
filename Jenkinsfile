    {  
      steps{
          echo "building the code here"
            }
    }
    stage("test")
    {  
      steps{
        echo "testing the code here"
            }
    }    
    stage("deploy")
    {  
      steps{
        echo "deploying the code here"
            }
    }    
  }
}
